<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="304.0" style="-fx-background-color: #121212;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.prj.ChatWindow">
   <children>
      <Button fx:id="exitButton" layoutX="266.0" layoutY="14.0" mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="24.0" text="X" />
      <TextArea fx:id="textArea" layoutX="17.0" layoutY="50.0" prefHeight="297.0" prefWidth="271.0" />
      <Label fx:id="chatLabel" layoutX="24.0" layoutY="18.0" prefHeight="17.0" prefWidth="184.0" text="Chatting with" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="textField" layoutX="17.0" layoutY="361.0" prefHeight="25.0" prefWidth="216.0" />
      <Button fx:id="sendButton" layoutX="245.0" layoutY="361.0" mnemonicParsing="false" onAction="#send" text="Send" />
      <Label fx:id="warningLabel" layoutX="37.0" layoutY="139.0" prefHeight="76.0" prefWidth="227.0" textFill="#f50000">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
